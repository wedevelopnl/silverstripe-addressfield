{
    "name": "thewebmen/silverstripe-addressfield",
    "description": "Silverstripe address field with geolocation",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "geolocation",
        "form",
        "field",
        "address"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "jeroendesloovere/geolocation-php-api": "1.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-addressfield",
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "resources"
        ]
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\Addressfield\\": "src/"
        }
    }
}
