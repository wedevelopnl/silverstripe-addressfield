{
    "name": "wedevelopnl/silverstripe-addressfield",
    "description": "Silverstripe module to create nested menus",
    "keywords": [
        "silverstripe",
        "menu",
        "nested",
        "links"
    ],
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "jeroendesloovere/geolocation-php-api": "1.3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "extra": {
        "installer-name": "silverstripe-addressfield",
        "branch-alias": {
            "dev-main": "3.x-dev"
        },
        "expose": [
            "resources"
        ]
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\Addressfield\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false,
            "silverstripe/recipe-plugin": false
        }
    }
}
